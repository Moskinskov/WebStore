@model IEnumerable<WebStore.Model.Employee>

<div class="container">
    <h3>Сотрудники</h3>
    <table class="table">
        <thead>
        <tr>
            <th>ID</th>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Возраст</th>
            <th>Доп.инфо</th>
        </tr>
        </thead>
        <tbody>
        @foreach (Employee employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.FirstName</td>
                <td>@employee.Patronymic</td>
                <td>@employee.Age</td>
                <td>@employee.LastName</td>
                <td>
                    <a asp-action="Details" asp-route-id="@employee.Id">подробнее</a>
                </td>
            </tr>
        }
        </tbody>
    </table>

    <a href="/">На главную</a>
</div>